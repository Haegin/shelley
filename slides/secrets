title Bonus secrets

read; echo -en '\t\e[0;34m$>\e[1;37m $EDITOR directory/file'
read -s; echo -en '\r\t\e[1;93m[Esc+q]\e[1;37m                                  '
read -s; echo -en '\r\t\e[0;34m$>\e[1;37m                                      '
read -s; echo -en '\r\t\e[0;34m$>\e[1;37m mkdir directory                      '
read -s; echo -en '\r\t\e[0;34m$>\e[1;37m $EDITOR directory/file               '

read
